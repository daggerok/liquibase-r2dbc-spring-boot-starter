        <rule groupId="io.vavr" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">.*-alpha-*</ignoreVersion>
            </ignoreVersions>
        </rule>
    </rules> -->
</ruleset>
